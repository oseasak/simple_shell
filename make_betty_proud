/**
 * _puts - prints a string to stdout
 * @str: the string to be printed
 *
 * Return: none
 */
void _puts(char *str)
{
        int i;

        for (i = 0; str[i] != '\0'; i++)
                putchar(str[i]);
}

/**
 * main - entry point of the program
 *
 * Return: always 0 (success)
 */
int main(void)
{
        char *str = "Hello, world!";

        _puts(str);

        return (0);
}
